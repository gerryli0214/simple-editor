{
  "compilerOptions": {
    "incremental": true, // 增量编译
    "allowJs": true,
    "lib": [ // 内置对象所对应的标准库
      "es6",
      "dom",
      "es2017" 
    ],
    "sourceMap": true, // 调试ts源码
    "downlevelIteration": true,
    "target": "ES5" // 编译JS目标版本
  },
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "src/styles"
  ]
}